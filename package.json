{
  "name": "nline-blog",
  "version": "1.0.0",
  "private": true,
  "description": "nLine's Notion powered blog",
  "author": "nLine <info@nline.io>, Travis Fischer <travis@transitivebullsh.it>",
  "repository": "nline/nline-blog",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && pnpm add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && pnpm link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm rebuild sharp"
  },
  "dependencies": {
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@keyvhq/core": "^2.1.1",
    "@keyvhq/redis": "^2.1.5",
    "@vercel/analytics": "^1.3.1",
    "@vercel/og": "^0.6.2",
    "canvas": "^2.11.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "eslint-config-next": "^14.2.5",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.7.2",
    "got": "^14.4.2",
    "isomorphic-unfetch": "^4.0.2",
    "lqip-modern": "^2.1.0",
    "next": "^14.2.5",
    "notion-client": "^6.16.0",
    "notion-types": "^6.16.0",
    "notion-utils": "^6.16.0",
    "p-map": "^7.0.2",
    "p-memoize": "^7.1.1",
    "posthog-js": "^1.157.0",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-body-classname": "^1.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-notion-x": "^6.16.0",
    "react-tweet-embed": "^2.0.0",
    "react-use": "^17.5.1",
    "rss": "^1.2.2",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/got": "^9.6.12",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.7.1"
}